	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee 
	http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd">
  <servlet>
    <servlet-name>ProvinceServlet</servlet-name>
    <servlet-class>SQLservlet.ProvinceServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginservlet</servlet-name>
    <servlet-class>SQLservlet.loginservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>userservlet</servlet-name>
    <servlet-class>SQLservlet.userservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>cityServlet</servlet-name>
    <servlet-class>SQLservlet.cityServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>bankservlet</servlet-name>
    <servlet-class>SQLservlet.bankservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>managerServlet</servlet-name>
    <servlet-class>SQLservlet.managerServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>addressServlet</servlet-name>
    <servlet-class>SQLservlet.addressServlet</servlet-class>
  </servlet>
  <servlet>
