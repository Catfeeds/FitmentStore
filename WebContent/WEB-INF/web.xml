    <servlet-name>PingjiaServlet</servlet-name>
    <url-pattern>/pingjiaservlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CollectionServlet</servlet-name>
    <url-pattern>/collectionservlet</url-pattern>
  </servlet-mapping>
  <filter>
  	<filter-name>FilterEncoding</filter-name>
  	<filter-class>Filter.FilterEncoding</filter-class>
  	<init-param>
  		<param-name>config</param-name>
  		<param-value>utf-8</param-value>
  	</init-param>
  </filter>
  <filter-mapping>
  	<filter-name>FilterEncoding</filter-name>
  	<url-pattern>/*</url-pattern>
  </filter-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
