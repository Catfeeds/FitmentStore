	function resetGoodsSepc() {
		$.ajax({
			url: '/mll_api/api/goods_spec?goods_id='+26115,
			type: 'GET',
			dataType: 'json',
			cache: false,
			success: function(json) {
				if ( json && json.properties && json.properties.length > 0 ) {
					var html = [],
					$pro = json.properties;

					html.push('<table data-attr="update">');
					for ( var i = 0, ii = $pro.length; i < ii; i++ ) {
						var $item = $pro[i];
						var first = (i == 0) ? 'class="first"' : '';
						html.push('<tr '+first+'><th colspan="2">'+$item.name+'</th></tr>');
						for ( var j = 0, jj = $item.value.length; j < jj; j++ ) {
							$sutItem = $item.value[j];
							html.push('<tr>');
								html.push('<th class="key">'+$sutItem.name+'</th>');
								html.push('<td>'+$sutItem.value+'</td>');
							html.push('</tr>');
						}
					}
					html.push('</table>');

					guigeDom.html(html.join(''));
				}
			}
		});
	}
})(jQuery);
</script>

